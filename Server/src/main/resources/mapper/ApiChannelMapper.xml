<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xwdz.site.mapper.ApiChannelMapper">


    <!-- 查询所有 -->
    <select id="findAll" resultType="com.xwdz.site.entity.ApiChannel">
        SELECT * FROM api_channel
    </select>

    <select id="findByKey" parameterType="String" resultType="com.xwdz.site.entity.ApiChannel">

        SELECT * FROM api_channel WHERE `key` = #{key}

    </select>

</mapper>
